{
  "manifest_version": 2,

  "name": "Knurler",
  "description": "Testing it out,bruh",
  "version": "0.0",
  // Not too sure about this:
  "content_scripts": [{
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery-3.0.0.min.js", "pwds.js"],
      "run_at": "document_start"
    }],
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab","tabs","notifications"
   ],
    "web_accessible_resources": [
    "icon.png"
  ]
}