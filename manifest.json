{
  "manifest_version": 2,

  "name": "Knurler",
  "description": "Testing it out,bruh",
  "version": "0.3",
  "content_scripts": [{
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery-3.0.0.min.js", "pwds.js"],
      "run_at": "document_start"
    }],
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab","tabs","notifications","storage","webRequest",
          "https://potato-server.herokuapp.com/stuff"
   ],
  "externally_connectable" : {
    "matches": [ "https://potato-server.herokuapp.com/stuff" ]
  },
    "web_accessible_resources": [
    "icon.png"
  ]
}